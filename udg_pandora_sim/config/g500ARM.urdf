<?xml version="1.0"?>
<robot name="girona500">
  <!-- base link is the body of the robot -->
  <link name="base_link">
      <visual>
         <origin rpy="0.0 0 0.0" xyz="0.0 0 0.0"/>
         <geometry>
           <!-- <mesh filename="file://robot2/GIRONA500v2/g500.osg"/> -->
           <box size="1.20 0.8 0.6"/>
         </geometry>
         <material name="Yellow">
           <color rgba="0.9 0.9 0.0 1.0"/>
        </material>
       </visual>
   </link>

   <link name="base">
      <visual>
        <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
         <geometry>
           <mesh filename="package://udg_pandora_sim/data2/robot2/NEW_PANDORA_ARM/bb_part1.stl" />
         </geometry>
         <material name="Grey">
           <color rgba="0.5 0.5 0.5 1.0"/>
        </material>
      </visual>
   </link>

   <joint name="baselink_to_base" type="fixed">
      <parent link="base_link"/>
      <child link="base"/>
      <axis xyz="0 0 1"/>
      <origin rpy="0 0 0" xyz="0.5 0.0 0.4"/>
   </joint>


  <link name="shoulder">
      <visual>
         <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
         <geometry>
            <mesh filename="package://udg_pandora_sim/data2/robot2/NEW_PANDORA_ARM/bb_part2.stl" />
         </geometry>
         <material name="LightGrey">
           <color rgba="0.75 0.75 0.75 1.0"/>
        </material>
      </visual>
   </link>

   <joint name="base_to_shoulder" type="revolute">
      <parent link="base"/>
      <child link="shoulder"/>
      <axis xyz="0 0 1"/>
      <origin rpy="0 0 0" xyz="0.25 -0.02 0.0"/>
      <limit effort="30" lower="-0.21" upper="1.47" velocity="0.0547"/>
   </joint>


   <link name="biceps">
      <visual>
         <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
         <geometry>
            <mesh filename="package://udg_pandora_sim/data2/robot2/NEW_PANDORA_ARM/bb_part3.stl" />
         </geometry>
         <material name="Grey">
           <color rgba="0.5 0.5 0.5 1.0"/>
        </material>
      </visual>
   </link>

   <joint name="shoulder_to_biceps" type="revolute">
      <parent link="shoulder"/>
      <child link="biceps"/>
      <axis xyz="0 1 0"/>
      <origin rpy="0 0 0" xyz="0.10 0.0 0.0"/>
      <limit effort="30" lower="-0.58" upper="1.606" velocity="0.08327"/>
   </joint>

  <link name="forearm">
      <visual>
         <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
         <geometry>
            <mesh filename="package://udg_pandora_sim/data2/robot2/NEW_PANDORA_ARM/bb_part4.stl" />
         </geometry>
         <material name="LightGrey">
           <color rgba="0.75 0.75 0.75 1.0"/>
        </material>
      </visual>
   </link>

   <joint name="elbow" type="revolute">
      <parent link="biceps"/>
      <child link="forearm"/>
      <axis xyz="0 1 0"/>
      <origin rpy="0 -1.57 0" xyz="0.22 0.0 0.05"/>
      <limit effort="30" lower="-1.46" upper="0.78" velocity="0.0605"/>
   </joint>

  <link name="palm">
      <visual>
         <origin rpy="0 0 0" xyz="0.0 0 0.0"/>
         <geometry>
            <mesh filename="package://udg_pandora_sim/data2/robot2/NEW_PANDORA_ARM/bb_part5.stl" />
         </geometry>
         <material name="LightBlack">
           <color rgba="0.2 0.2 0.2 1.0"/>
        </material>
      </visual>
   </link>

   <joint name="wrist" type="continuous">
      <parent link="forearm"/>
      <child link="palm"/>
      <axis xyz="1 0 0"/>
      <origin rpy="0 0 0" xyz="0.2 0.01 -0.1"/>
      <limit effort="30" lower="-3.14" upper="3.12" velocity="1.5305"/>
   </joint>



  <link name="jaw">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.0"/>
      <geometry>
        <mesh filename="package://udg_pandora_sim/data2/robot2/NEW_PANDORA_ARM/bb_jaw.stl"/>
      </geometry>
         <material name="LightBlack2">
           <color rgba="0.3 0.3 0.2 1.0"/>
        </material>
    </visual>
  </link>

  <joint name="palm_to_jawS" type="fixed">
    <parent link="palm"/>
    <child link="jaw"/>
    <axis xyz="0 0 1"/>
    <origin rpy="0 0 0.0" xyz="0.08 -0.03 0.0"/>
  </joint>

<!--
  <link name="jawI">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://udg_pandora/data2/robot2/NEW_PANDORA_ARM/jawI.osg"/>
      </geometry>
    </visual>
  </link>

  <joint name="support_to_jaw2" type="continuous">
    <parent link="palm"/>
    <child link="jawI"/>
    <axis xyz="0 0 -1"/>
    <origin rpy="0 0 -0.5" xyz="0.08 -0.03 0.0"/>
    <mimic joint="palm_to_jawS" multiplier="1" offset="0"/>
  </joint>
-->

</robot>
